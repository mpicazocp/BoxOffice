{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "react/prop-types": 0,
      "react/react-in-jsx-scope": "off",
      "linebreak-style": ["error", "unix"] // change 'windows' to 'unix' if not on Windows OS
    }
}
